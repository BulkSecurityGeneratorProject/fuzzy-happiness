<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Exemplaire.
    -->
    <changeSet id="20161010150033-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="livre_id"
                                 baseTableName="exemplaire"
                                 constraintName="fk_exemplaire_livre_id"
                                 referencedColumnNames="id"
                                 referencedTableName="livre"/>

        <addForeignKeyConstraint baseColumnNames="magazine_id"
                                 baseTableName="exemplaire"
                                 constraintName="fk_exemplaire_magazine_id"
                                 referencedColumnNames="id"
                                 referencedTableName="magazine"/>

        <addForeignKeyConstraint baseColumnNames="reservation_id"
                                 baseTableName="exemplaire"
                                 constraintName="fk_exemplaire_reservation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reservation"/>

    </changeSet>
</databaseChangeLog>
